<!ELEMENT TimeML ANY><!ELEMENT EVENT (#PCDATA)*><!ATTLIST EVENT           eid ID #REQUIRED          class (OCCURRENCE|PERCEPTION|REPORTING|ASPECTUAL|STATE|I_STATE|I_ACTION|MODAL) #REQUIRED          tense (PAST|PRESENT|FUTURE|NONE) #REQUIRED          aspect (PROGRESSIVE|PERFECTIVE|PERFECTIVE_PROGRESSIVE|NONE) #REQUIRED><!ELEMENT TIMEX3 (#PCDATA)><!ATTLIST TIMEX3           tid ID #REQUIRED          type (DATE|TIME|DURATION) #REQUIRED          functionInDocument (CREATION_TIME|EXPIRATION_TIME|MODIFICATION_TIME|PUBLICATION_TIME|RELEASE_TIME|RECEPTION_TIME|NONE) "NONE"          temporalFunction (true|false) "false"          value CDATA #REQUIRED          valueFromFunction IDREF #IMPLIED          mod (BEFORE|AFTER|ON_OR_BEFORE|ON_OR_AFTER|LESS_THAN|MORE_THAN|EQUAL_OR_LESS|EQUAL_OR_MORE|START|MID|END|APPROX) #IMPLIED          anchorTimeID IDREF #IMPLIED          anchorEventID IDREF #IMPLIED><!ELEMENT SIGNAL (#PCDATA)*><!ATTLIST SIGNAL           sid ID #REQUIRED><!ELEMENT MAKEINSTANCE EMPTY><!ATTLIST MAKEINSTANCE          eiid ID #REQUIRED          eventID IDREF #REQUIRED          signalID IDREF #IMPLIED          cardinality CDATA #IMPLIED><!ELEMENT TLINK EMPTY><!ATTLIST TLINK           eventInstanceID IDREF #IMPLIED          timeID IDREF #IMPLIED          signalID IDREF #IMPLIED          relatedToTime IDREF #IMPLIED          relatedToEvent IDREF #IMPLIED          relType (BEFORE|AFTER|INCLUDES|IS_INCLUDED|HOLDS|SIMULTANEOUS|IAFTER|IBEFORE|IDENTITY|BEGINS|ENDS|BEGUN_BY|ENDED_BY) #REQUIRED          magnitude IDREF #IMPLIED><!ELEMENT SLINK EMPTY><!ATTLIST SLINK           eventInstanceID IDREF #IMPLIED          subordinatedEvent IDREF #IMPLIED          subordinatedEventInstance IDREF #IMPLIED          signalID IDREF #IMPLIED          relType (MODAL|NEGATIVE|EVIDENTIAL|NEG_EVIDENTIAL|FACTIVE|COUNTER_FACTIVE) #REQUIRED><!ELEMENT ALINK EMPTY><!ATTLIST ALINK           eventInstanceID IDREF #REQUIRED          signalID IDREF #IMPLIED          relatedToEvent IDREF #REQUIRED          relType (INITIATES|CULMINATES|TERMINATES|CONTINUES|REINITIATES) #REQUIRED><!ELEMENT CONFIDENCE EMPTY><!ATTLIST CONFIDENCE           tagType (EVENT|TIMEX3|SIGNAL|MAKEINSTANCE|TLINK|SLINK|ALINK) #REQUIRED          tagID IDREF #REQUIRED          attributeName (anchorEventID|anchorTimeID|aspect|cardinality|class|eventID|eventInstanceID|functionInDocument|magnitude|mod|relType|relatedToEvent|signalID|subordinatedEvent|subordinatedEventInstance|temporalFunction|tense|timeID|type|value|valueFromFunction) #IMPLIED          confidenceValue CDATA #REQUIRED>